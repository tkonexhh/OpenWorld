				float2 texCoord69 = IN.ase_texcoord7.xy * float2( 1,1 ) + float2( 0,0 );
				float2 texCoord60 = IN.ase_texcoord7.xy * float2( 1,1 ) + float2( 0,0 );
				float3 tanToWorld0 = float3( WorldTangent.x, WorldBiTangent.x, WorldNormal.x );
